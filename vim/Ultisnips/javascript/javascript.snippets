snippet log "Console log"
console.log('$1');
endsnippet

snippet elog "Console log no string"
console.log($1);
endsnippet

snippet tl "Test console log"
console.log('Test');
endsnippet

snippet forl "For loop"
for (let i = 0; i < $1; i += 1) {
	$2
}
endsnippet

snippet if "if statement"
if ($1) {
	$2
}
endsnippet
